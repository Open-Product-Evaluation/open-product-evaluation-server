{
  "name": "open-product-evaluation-server",
  "version": "0.1.0",
  "description": "Open Product Evaluation Server is a service that provides an API to evaluate different products e.g. apps or websites",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest -i",
    "build": "cd admin && npm run build",
    "serve": "cd admin && npm run serve",
    "mongo": "mongod --logpath ./mongodb.log",
    "seed": "node seeds/seeder.js",
    "postinstall": "cd admin && npm install && npm run build",
    "test:e2e": "cd admin && npm run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/th-koeln/open-product-evaluation-server.git"
  },
  "author": "Fabian Daugs, Dennis Dubbert Sascha Lemke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/th-koeln/open-product-evaluation-server/issues"
  },
  "homepage": "https://github.com/th-koeln/open-product-evaluation-server#readme",
  "dependencies": {
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "fs-extra": "6.0.1",
    "graphql-iso-date": "^3.5.0",
    "graphql-shield": "2.2.6",
    "graphql-voyager": "^1.0.0-rc.15",
    "graphql-yoga": "^1.14.7",
    "jsonwebtoken": "8.3.0",
    "merge-graphql-schemas": "^1.5.1",
    "mongodb": "3.0.10",
    "mongoose": "5.1.5",
    "shortid": "2.2.8",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "jest": "23.6.0",
    "mongo-seeding": "3.0.0-0",
    "nodemon": "1.17.5",
    "supertest": "3.1.0",
    "wait-on": "3.0.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testRegex": "./test/.*.test.js$",
    "verbose": true
  }
}
