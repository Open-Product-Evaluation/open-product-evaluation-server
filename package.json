    "@vue/test-utils": "1.0.0-beta.28",
    "node-sass": "4.11.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "2.5.21"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/api/setup.js",
    "testRegex": "./tests/api/.*.test.js$",
    "verbose": true
  }
}
